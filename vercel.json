{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "projectautoentreprise-.*-kebcis-projects.vercel.app"
        }
      ],
      "destination": "https://proflow-biz.vercel.app/$1",
      "permanent": false,
      "statusCode": 302
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "projectautoentreprise-kebcis-projects.vercel.app"
        }
      ],
      "destination": "https://proflow-biz.vercel.app/$1",
      "permanent": false,
      "statusCode": 302
    }
  ]
}
